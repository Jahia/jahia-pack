<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
   <!-- 
    FALSE POSITIVE: GraphQL Java CPE Matching for Jahia Modules
    
    All modules under org.jahia.modules have proper vendor metadata in MANIFEST.MF:
    - Implementation-Vendor: Jahia Solutions Group SA
    - Bundle-Vendor: Jahia Solutions Group SA
    
    OWASP dependency-check still generates false CPE matches when module versions
    coincidentally match vulnerable GraphQL Java versions.
    
    Our actual GraphQL Java dependency: 21.5 (not vulnerable to CVE-2022-37734)
    CVE-2022-37734 affects: graphql-java < 19.0
    -->
    <!-- Suppress graphql-java false positives for ALL Jahia modules -->
    <suppress>
        <notes><![CDATA[
        FALSE POSITIVE: Jahia modules incorrectly matched to graphql-java
        
        Modules have correct vendor metadata but version numbers coincide with
        vulnerable GraphQL Java versions, triggering false CPE matches.
        
        Affected modules:
        - external-provider (4.9.0)
        - jahia-dashboard (1.12.0)
        - personal-api-tokens (1.6.0)
        - security-filter-tools (2.5.0)
        
        Our actual GraphQL dependency: graphql-java:21.5 (not vulnerable)
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.jahia\.modules/.*$</packageUrl>
        <cpe>cpe:/a:graphql-java_project:graphql-java</cpe>
    </suppress>
    
    <!-- Suppress generic "service" false positives -->
    <suppress>
        <notes><![CDATA[
        FALSE POSITIVE: Generic service_project:service CPE match
        
        Modules with generic names trigger matches to cpe:/a:service_project:service
        These are Jahia-specific modules, not the generic "service" project.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.jahia\.modules/.*$</packageUrl>
        <cpe>cpe:/a:service_project:service</cpe>
    </suppress>
    
    <!-- Suppress generic "service" false positives -->
    <suppress>
        <notes><![CDATA[
        FALSE POSITIVE: ckeditor CPE is matched with our richtext-ckeditor5 module
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.jahia\.modules/richtext-ckeditor5@.*$</packageUrl>
        <cpe>cpe:/a:ckeditor:ckeditor</cpe>
    </suppress>
</suppressions>